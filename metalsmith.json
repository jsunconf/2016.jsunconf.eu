{
  "metadata": {
    "title": "jsunconf 2016",
    "description": "The unconference that started it all."
  },
  "source": "content",
  "plugins": {
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-templates": {
      "engine": "jade",
      "directory": "theme/jade"
    },
    "metalsmith-assets": {
      "source": "./theme/assets",
      "destination": "./"
    },
    "metalsmith-stylus": {
      "paths": ["theme/styles"]
    }
  }
}
