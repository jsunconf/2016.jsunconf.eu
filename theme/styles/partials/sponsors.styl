.sponsors
  font-size: 1rem
  &__title
    text-align: center
    margin-bottom: emCalc(60px)
  &__level
    margin-bottom: emCalc(70px)
    text-align: center
    &__name
      margin-bottom: emCalc(10px)
    &__logos
      display: flex
      justify-content: space-around
      align-items: center
      flex-wrap: wrap
      &__link
        flex-shrink: 0
        height: emCalc(60px)
        margin-bottom: 1em
        border-bottom-color: transparent
        &--Silver
          height: emCalc(50px)
        &--Bronze
          height: emCalc(40px)
        img
          height: 100%
