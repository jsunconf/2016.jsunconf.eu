extends _layout

block main

  include index-partials/header
  include index-partials/mailchimp
  include index-partials/tickets
  include index-partials/date
  include index-partials/sponsors
  include index-partials/awesome
  include index-partials/code-of-conduct

  each level in ['Gold', 'Silver', 'Bronze']
    - var levelSponsors = sponsors.filter(sponsor => sponsor.level === level);

    if levelSponsors && levelSponsors.length > 0
      h4= level
      each sponsor in levelSponsors
        img(src="/images/sponsors/"+sponsor.image, alt=sponsor.name)
